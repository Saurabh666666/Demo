{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Message",
						"value": "Copy Activity is successful"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "SqlDWSink",
						"allowPolyBase": true,
						"polyBaseSettings": {
							"rejectValue": 0,
							"rejectType": "value",
							"useTypeDefault": true
						},
						"tableOption": "autoCreate"
					},
					"enableStaging": true,
					"stagingSettings": {
						"linkedServiceName": {
							"referenceName": "newStorage",
							"type": "LinkedServiceReference"
						},
						"path": "demo"
					}
				},
				"inputs": [
					{
						"referenceName": "NewStorage",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SynapseAnalytics",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}